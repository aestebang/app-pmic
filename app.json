{
  "expo": {
    "name": "APP PMIC 10",
    "slug": "APP PMIC 10",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icono.png",
    "scheme": "bushersapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icono.png",
      "resizeMode": "contain",
      "backgroundColor": "transparent"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.bushers.bushersapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icono.png"
      }
    }
  }
}
